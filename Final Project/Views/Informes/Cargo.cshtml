@model  List<CapaEntidad.Cargo>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}





<table class="table table-dark">
    <thead>
        <tr>
            <th>ID</th>
            <th> Nombre del Cargo </th>
            

        </tr>
    </thead>


    <tbody>
        @foreach (var i in Model)
        {
            <tr>
                <td>@i.Id_DelCargo</td>
                <td>@i.Nombre_Del_Cargo</td>
            </tr>
        }
    </tbody>
</table>
